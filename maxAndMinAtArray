public class Main {
    public static void main(String[] args) {
        int[] list = {56, 25, 14, 29, 78, 1};
        int min = list[0];
        int max = list[0];
        for (int number : list) {
            if (number < min) {
                min = number;
            }
            if (number > max) {
                max = number;
            }
        }
        System.out.println("Minimum Value:" + min);
        System.out.println("Maximum Value:" + max);

    }
}
